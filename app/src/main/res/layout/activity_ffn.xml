        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.177" />

    <Button
        android:id="@+id/FF20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FF 20 "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/FF30"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/FF10"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.177" />

    <Button
        android:id="@+id/FF30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FF 30"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/FF20"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.177" />

    <Button
        android:id="@+id/FF40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FF 40"
        app:layout_constraintBottom_toTopOf="@+id/FF130"
        app:layout_constraintEnd_toStartOf="@+id/FF50"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/FF10" />

    <Button
        android:id="@+id/FF50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FF 50"
        app:layout_constraintBottom_toTopOf="@+id/FF140"
        app:layout_constraintEnd_toStartOf="@+id/FF70"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/FF40"
        app:layout_constraintTop_toBottomOf="@+id/FF20" />


</androidx.constraintlayout.widget.ConstraintLayout>