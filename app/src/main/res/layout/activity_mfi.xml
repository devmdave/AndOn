<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".activity_mfi">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="418dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/purple"
            android:fontFamily="serif"
            android:text="Main Frame Inner"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="34sp"
            android:textStyle="bold" />
    </FrameLayout>

    <Button
        android:id="@+id/MF10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="MF # 10"
        app:layout_constraintBottom_toTopOf="@+id/MF40"
        app:layout_constraintEnd_toStartOf="@+id/MF20"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="MF # 20"
        app:layout_constraintBottom_toTopOf="@+id/MF50"
        app:layout_constraintEnd_toStartOf="@+id/MF30"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/MF10"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

    <Button
        android:id="@+id/MF30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:text="MF # 30"
        app:layout_constraintBottom_toTopOf="@+id/MF60"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/MF20"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MF # 40"
        app:layout_constraintBottom_toTopOf="@+id/MF70"
        app:layout_constraintEnd_toStartOf="@+id/MF50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MF10"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MF # 50"
        app:layout_constraintBottom_toTopOf="@+id/MF80"
        app:layout_constraintEnd_toStartOf="@+id/MF60"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/MF40"
        app:layout_constraintTop_toBottomOf="@+id/MF20"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MF # 60"
        app:layout_constraintBottom_toTopOf="@+id/MF90"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/MF50"
        app:layout_constraintTop_toBottomOf="@+id/MF30"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MF # 70"
        app:layout_constraintBottom_toTopOf="@+id/MF100"
        app:layout_constraintEnd_toStartOf="@+id/MF80"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MF40"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF80"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MF # 80"
        app:layout_constraintBottom_toTopOf="@+id/MF110"
        app:layout_constraintEnd_toStartOf="@+id/MF90"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/MF70"
        app:layout_constraintTop_toBottomOf="@+id/MF50" />

    <Button
        android:id="@+id/MF90"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MF # 90"
        app:layout_constraintBottom_toTopOf="@+id/MF120"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/MF80"
        app:layout_constraintTop_toBottomOf="@+id/MF60"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="150dp"
        android:text="MF # 100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/MF110"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MF70"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF110"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="150dp"
        android:text="MF # 110"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/MF120"
        app:layout_constraintStart_toEndOf="@+id/MF100"
        app:layout_constraintTop_toBottomOf="@+id/MF80"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF120"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="150dp"
        android:text="MF # 120"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/MF110"
        app:layout_constraintTop_toBottomOf="@+id/MF90"
        app:layout_constraintVertical_chainStyle="spread" />


</androidx.constraintlayout.widget.ConstraintLayout>