<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".activity_mfo">

    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="418dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/purple"
            android:fontFamily="serif"
            android:text="Main Frame Outter"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="34sp"
            android:textStyle="bold" />
    </FrameLayout>

    <Button
        android:id="@+id/MF0125"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MFO 125"
        app:layout_constraintBottom_toTopOf="@+id/MFO150"
        app:layout_constraintEnd_toStartOf="@+id/MF0130"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MF0130"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MFO 130"
        app:layout_constraintBottom_toTopOf="@+id/MFO160"
        app:layout_constraintEnd_toStartOf="@+id/MFO140"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/MF0125"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MFO140"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MFO 140"
        app:layout_constraintBottom_toTopOf="@+id/MFO170"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/MF0130"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MFO150"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MFO 150"
        app:layout_constraintBottom_toTopOf="@+id/MFO180"
        app:layout_constraintEnd_toStartOf="@+id/MFO160"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MF0125"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MFO160"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MFO 160"
        app:layout_constraintBottom_toTopOf="@+id/MFO190"
        app:layout_constraintEnd_toStartOf="@+id/MFO170"
        app:layout_constraintStart_toEndOf="@+id/MFO150"
        app:layout_constraintTop_toBottomOf="@+id/MF0130"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MFO170"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MFO 170"
        app:layout_constraintBottom_toTopOf="@+id/MFO200"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/MFO160"
        app:layout_constraintTop_toBottomOf="@+id/MFO140"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MFO180"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="200dp"
        android:text="MFO 180"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/MFO190"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MFO150"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MFO190"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="200dp"
        android:text="MFO 190"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/MFO200"
        app:layout_constraintStart_toEndOf="@+id/MFO180"
        app:layout_constraintTop_toBottomOf="@+id/MFO160"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/MFO200"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="200dp"
        android:text="MFO 200"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/MFO190"
        app:layout_constraintTop_toBottomOf="@+id/MFO170"
        app:layout_constraintVertical_chainStyle="spread" />


</androidx.constraintlayout.widget.ConstraintLayout>