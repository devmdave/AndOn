<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".activity_ubg">


    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="418dp"
        android:layout_height="70dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/purple"
            android:fontFamily="serif"
            android:text="Under Body GEO"
            android:textAlignment="center"
            android:textColor="@color/white"
            android:textSize="34sp"
            android:textStyle="bold" />
    </FrameLayout>


    <Button
        android:id="@+id/UBG01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UBG # 01"
        app:layout_constraintBottom_toTopOf="@+id/UBG20"
        app:layout_constraintEnd_toStartOf="@+id/UBG05"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/frameLayout"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG05"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UBG # 05"
        app:layout_constraintBottom_toTopOf="@+id/UBG30"
        app:layout_constraintEnd_toStartOf="@+id/UBG10"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/UBG01"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UBG # 10"
        app:layout_constraintBottom_toTopOf="@+id/UBG40"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/UBG05"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UBG # 20"
        app:layout_constraintBottom_toTopOf="@+id/UBG50"
        app:layout_constraintEnd_toStartOf="@+id/UBG30"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UBG01"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UBG # 30"
        app:layout_constraintBottom_toTopOf="@+id/UBG60"
        app:layout_constraintEnd_toStartOf="@+id/UBG40"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/UBG20"
        app:layout_constraintTop_toBottomOf="@+id/UBG05"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UBG # 40"
        app:layout_constraintBottom_toTopOf="@+id/UBG70"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/UBG30"
        app:layout_constraintTop_toBottomOf="@+id/UBG10"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GBG # 50"
        app:layout_constraintBottom_toTopOf="@+id/UBG80"
        app:layout_constraintEnd_toStartOf="@+id/UBG60"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UBG20"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG60"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UBG # 60"
        app:layout_constraintBottom_toTopOf="@+id/UBG90"
        app:layout_constraintEnd_toStartOf="@+id/UBG70"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/UBG50"
        app:layout_constraintTop_toBottomOf="@+id/UBG30"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG70"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="UBG # 70"
        app:layout_constraintBottom_toTopOf="@+id/UBG100"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/UBG60"
        app:layout_constraintTop_toBottomOf="@+id/UBG40"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG80"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="150dp"
        android:text="UBG # 80"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/UBG90"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/UBG50"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG90"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="150dp"
        android:text="UBG # 90"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/UBG100"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/UBG80"
        app:layout_constraintTop_toBottomOf="@+id/UBG60"
        app:layout_constraintVertical_chainStyle="spread" />

    <Button
        android:id="@+id/UBG100"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="150dp"
        android:text="UBG # 100"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:layout_constraintStart_toEndOf="@+id/UBG90"
        app:layout_constraintTop_toBottomOf="@+id/UBG70"
        app:layout_constraintVertical_chainStyle="spread" />

</androidx.constraintlayout.widget.ConstraintLayout>